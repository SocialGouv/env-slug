{
  "name": "@socialgouv/env-slug",
  "description": "Generate slug for K8S namespace or DB name from git branch",
  "version": "1.1.2",
  "author": "Fabrique numérique des Ministères Sociaux <dsi-incubateur@sg.social.gouv.fr> (https://incubateur.social.gouv.fr)",
  "bugs": "https://github.com/SocialGouv/env-slug/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.18.2",
    "@oclif/plugin-help": "3.2.4",
    "@oclif/errors": "^1.3.5",
    "@oclif/parser": "^3.8.6",
    "slugify": "^1.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@socialgouv/eslint-config-recommended": "^1.100.0",
    "@socialgouv/eslint-config-typescript": "^1.100.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "jest": "^27.4.0",
    "lint-staged": "^12.1.2",
    "nyc": "^15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{j,t}s": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "homepage": "https://github.com/SocialGouv/env-slug#readme",
  "keywords": [
    "env-slug",
    "slug",
    "gitlab"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**"
  ],
  "repository": "SocialGouv/env-slug.git",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc.yml",
    "build": "tsc -p tsconfig.build.json",
    "e2e": "jest -c ./e2e/jest.config.js",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "test": "jest",
    "typecheck": "tsc"
  },
  "oclif": {
    "bin": "env-slug"
  },
  "bin": {
    "env-slug": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  }
}
